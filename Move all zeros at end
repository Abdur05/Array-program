import java.util.*;
class Move0End
{
public static void main(String arg[])
{
int a[]={1,2,6,0,5,0,7,0,9};
int n=a.length;
int temp=0;
for(int i=0;i<n;i++)
{
for(int j=0;j<n;j++)
 {
if(a[i]!=0)
 {
temp=a[i];
a[i]=a[j];
a[j]=temp;
 }
 }
}
System.out.println("Modified array is : ");
for(int i=0;i<n;i++)
{
System.out.print(a[i]+" ");
}
}
}
